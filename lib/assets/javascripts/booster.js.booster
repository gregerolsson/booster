exports.Index = booster.View.extend({
  initialize: function() {
    this.render();
  },

  render: function() {
    $(this.el).html(index(this.collection))
  }
});

@@ layout
  <h1 data-yield="title"></h1>
  <div data-yield="content"></div>

@@ index
  <ul class="list-view">
    {{#each models}}
      <li>{{name}}</li>
    {{/each}}
  </ul>